<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Emag\ScmFaqBundle\Entity\Questions" table="questions">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="shortVersionQuestion" type="string" column="short_version_question" length="250" nullable="false"/>
    <field name="longVersionQuestion" type="text" column="long_version_question" nullable="true"/>
    <field name="askedByUserId" type="integer" column="asked_by_user_id" nullable="true"/>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="modified" type="datetime" column="modified" nullable="false"/>
  </entity>
</doctrine-mapping>
